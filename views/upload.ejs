<html>
  <body>
    <% if (locals.user) {%>
    <h1>Upload a file</h1>
    <form method="POST" action="/upload" enctype="multipart/form-data">
        <div>
            <input type="file" name="file">
        </div>
        <button type="submit">Submit</button>
    </form>
    <% } else { %>
    <h1>Please log in</h1>
    <% } %>
    <a href="/">Home</a>
  </body>
</html>